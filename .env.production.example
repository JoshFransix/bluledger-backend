# Neon Database
DATABASE_URL="postgresql://username:password@ep-xxxx-xxxx.region.aws.neon.tech/neondb?sslmode=require"

# Redis Cloud
REDIS_URL="redis://default:your-redis-password@redis-xxxxx.c123.region.ec2.cloud.redislabs.com:12345"

# JWT Secrets - CHANGE THESE IN PRODUCTION!
JWT_SECRET="your-super-secret-jwt-key-change-this"
REFRESH_TOKEN_SECRET="your-super-secret-refresh-key-change-this"
JWT_EXPIRES_IN="15m"
REFRESH_TOKEN_EXPIRES_IN="7d"

# Server Configuration
PORT=3001
NODE_ENV=production

# Frontend URL - Update with your actual frontend domain
FRONTEND_URL="https://your-frontend-domain.vercel.app"
